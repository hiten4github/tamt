<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:importm="urn:import:org.worldbank.transport.tamt.client.importer">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.outerBorder {
			border: 6px solid green;
			padding: 10px;
		}
		.map {
			border: 1px solid black;	
		}
		.info {
			border: 1px solid black;
			margin-left: 10px;
			height: 480px;
		}
		.tabs {
			margin-right: 10px;
		}
		.shadow {
			
		}
		.buffer {
			height: 500px;
			width: 900px;	
		}
	</ui:style>
	<g:HorizontalPanel styleName="{style.outerBorder}">
		<g:TabLayoutPanel ui:field="tabLayoutPanel" barUnit="PX" barHeight="20" width="900px" height="500px">
			<g:tab>
				<g:header>Upload</g:header>
				<importm:GPSTracesView ui:field="gpsTracesView"/>
			</g:tab>
		</g:TabLayoutPanel>
	</g:HorizontalPanel>
	
</ui:UiBinder> 